<div class="pushing__div--below col s4">
  <h5>Top 5 ranking</h5>
  <div class="collection">
    <% skaters.each do |skater| %>
      <a href="<%= skater_path(skater)%>" class="collection-item"><img class="navbar--title rounded--avatars" style="width:35px; height:35px;" src="<%= skater.logo.url(:thumb)%>" alt="Skater's avatar"> <%= skater.username %>
      <span class="navbar--title badge">18 likes</span></a>
    <%end%>
  </div>
</div>

<div class="col s8 pushing__div--below">
  <img class="winner--image" src="http://i.imgur.com/38CPPcu.jpg" width="525px"alt="Number one" align="left">
</div>
