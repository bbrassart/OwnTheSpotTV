    <div class="col-lg-5">
      <div class="media">
        <div class="media-left">
          <img class="media-object" src="<%= @spot.logo.url(:thumb)%>" alt="Spot's avatar">
        </div>
        <div class="media-body">
          <h4 class="media-heading"><%= @spot.name %></h4>
          <%= @spot.description %>
        </div>
      </div>
    </div>

